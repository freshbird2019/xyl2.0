<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="xyl.cct.pojo.Clazz" table="class" schema="xyl">
        <id name="cid">
            <column name="cid" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="year">
            <column name="year" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="major">
            <column name="major" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="college">
            <column name="college" sql-type="varchar(20)" length="20"/>
        </property>

    </class>
</hibernate-mapping>